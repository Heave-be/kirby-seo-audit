{
  "name": "kirby-seo-audit",
  "type": "module",
  "version": "1.1.0",
  "private": true,
  "packageManager": "pnpm@8.15.3",
  "scripts": {
    "dev": "PLAYGROUND=true kirbyup serve src/panel/index.js",
    "build": "kirbyup src/panel/index.js",
    "build:assets": "vite build --config src/assets/yoastseo/vite.config.js && vite build --config src/assets/yoastseo-translations/vite.config.js && vite build --config src/assets/jed/vite.config.js && vite build --config src/assets/string-pixel-width/vite.config.js",
    "build:playground": "PLAYGROUND=true pnpm run build && pnpm run build:assets",
    "readme:dev": "vitepress dev readme",
    "readme:build": "vitepress build readme",
    "readme:preview": "vitepress preview readme",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"src/panel/**\" --write",
    "archive": "pnpm run build && pnpm run build:assets && scripts/archive.sh",
    "release": "bumpp --all"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "autoprefixer": "^10.4.17",
    "bumpp": "^9.3.0",
    "eslint": "^8.57.0",
    "jed": "^1.1.1",
    "just-safe-get": "^4.2.0",
    "kirbyup": "^3.1.4",
    "kirbyuse": "^0.4.1",
    "ohash": "^1.1.3",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "sass": "^1.71.1",
    "string-pixel-width": "^1.11.0",
    "tailwindcss": "^3.4.1",
    "ufo": "^1.4.0",
    "vite": "^5.1.4",
    "vite-plugin-node-polyfills": "^0.21.0",
    "yoastseo": "^1.91.2"
  },
  "pnpm": {
    "overrides": {
      "node-sass": "sass"
    }
  }
}
